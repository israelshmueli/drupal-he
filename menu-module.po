msgid "disable"
msgstr "ביטול"

#: modules/menu.module:431
msgid "enable"
msgstr "אפשר"

#: modules/menu.module:438
msgid "reset"
msgstr "אתחול"

#: modules/menu.module:450;493
msgid "disabled"
msgstr "לא פעיל"

#: modules/menu.module:455
msgid "Yes"
msgstr "כן"

#: modules/menu.module:455
msgid "No"
msgstr "לא"

