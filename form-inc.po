#: includes/form.inc:396
msgid "Validation error, please try again. If this error persists, please contact the site administrator."
msgstr "בעית ולידציה, אנא נסה שוב. אם שגיאה זו ממשיכה להופיע, אנא פנה למנהל האתר."

#: includes/form.inc:521
msgid "!name field is required."
msgstr "חובה למלא את השדה !name."

#: includes/form.inc:526
msgid "!name cannot be longer than %max characters but is currently %length characters long."
msgstr "השדה !name לא יכול להכיל יותר מאשר %max תוים, אבל יש בו כרגע %length תוים."

#: includes/form.inc:542
msgid "Illegal choice %choice in !name element."
msgstr "אי אפשר לבחור '%choice' בפריט !name."

#: includes/form.inc:1092
msgid "Confirm password"
msgstr "אשר סיסמא"

#: includes/form.inc:1109
msgid "The specified passwords do not match."
msgstr "הסיסמאות שהזנת אינן תואמות."

#: includes/form.inc:1113
msgid "Password field is required."
msgstr "חובה למלא סיסמא."

#: includes/form.inc:1191
msgid "The specified date is invalid."
msgstr "התאריך שהוזן אינו תקף."

#: includes/form.inc:1507
msgid "This field is required."
msgstr "חובה למלא שדה זה."

#: includes/form.inc:1512;1515
msgid "!title: !required"
msgstr "!title: !required"

